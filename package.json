    "ping": "^0.4.1",
    "proxy-check": "^1.0.8",
    "proxy-verifier": "^0.5.0"
  },
  "scripts": {
    "start": "electron . ",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "dependencies": {
    "@electron/remote": "^2.0.1",
    "@treverix/remote": "^0.1.1",
    "proxy-checker": "git+https://github.com/newx666/proxy-checker.git",
