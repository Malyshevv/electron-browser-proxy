  "license": "MIT",
  "build": {
    "appId": "com.electron.app",
    "win": {
      "target": "portable"
    },
    "portable": {
      "unicode": false,
      "artifactName": "my_electron_app.exe"
    }
  },
  "devDependencies": {
    "axios": "^0.24.0",
    "custom-electron-titlebar": "^3.2.7",
    "electron": "^15.3.1",
